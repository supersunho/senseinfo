# .env.example
# Telegram API Credentials (Required)
# Get from https://my.telegram.org/apps
TELEGRAM_API_ID=12345678
TELEGRAM_API_HASH=your_api_hash_here_1234567890abcdef

# Database Configuration
POSTGRES_USER=infosense
POSTGRES_PASSWORD=secure_random_password_here
POSTGRES_DB=infosense
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Application Settings
SECRET_KEY=your_super_secret_key_here_change_in_production
SESSION_DIRECTORY=/app/sessions
MAX_CHANNELS_PER_ACCOUNT=500
MESSAGE_PROCESS_INTERVAL=5

# Proxy Configuration (Optional but Recommended)
# Format: socks5://user:pass@host:port or http://host:port
PROXY_LIST=socks5://user1:pass1@proxy1.example.com:1080,socks5://user2:pass2@proxy2.example.com:1080

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
CORS_ALLOW_CREDENTIALS=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
